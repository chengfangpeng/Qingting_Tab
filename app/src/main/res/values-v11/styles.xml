<resources>

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="MyAppTheme" parent="android:Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>


        <!-- API 11 theme customizations can go here. -->
    </style>

    <style name="Theme_Orange" parent="MyAppTheme">
        <item name="maincolor">@android:color/holo_orange_light</item>

    </style>
    <style name="Theme_Blue" parent="MyAppTheme">
        <item name="maincolor">@color/main_color</item>

    </style>

    <style name="Theme_Puple" parent="MyAppTheme">
        <item name="maincolor">@android:color/holo_purple</item>

    </style>

    <style name="Theme_Red" parent="MyAppTheme">
        <item name="maincolor">@android:color/holo_red_light</item>

    </style>

    <style name="Theme_Green" parent="MyAppTheme">
        <item name="maincolor">@android:color/holo_green_dark</item>

    </style>

</resources>
